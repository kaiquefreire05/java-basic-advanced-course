package entities;

public class Product {
	
	private String name;
	private Double price;
	
	public Product() {}
	
	
	public Product(String name, Double price) {
		super();
		this.name = name;
		this.price = price;
	}




	@Override
	public String toString() {
		return "Product [name=" + name + ", price=" + price + "]";
	}
	
	
}
